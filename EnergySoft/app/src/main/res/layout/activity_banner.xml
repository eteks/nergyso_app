<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">




    <android.support.v4.view.ViewPager
        android:layout_width="wrap_content"
        android:layout_height="250dp"
        android:id="@+id/viewPagerdash"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/menuicons"
        android:orientation="horizontal"
        android:layout_below="@+id/viewPagerdash"
        android:layout_marginBottom="208dp"
        android:layout_alignParentBottom="true">

        <ImageButton
            android:id="@+id/firstFragment"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/viewPagerdash"
            app:srcCompat="@drawable/birthday_tile" />

        <ImageButton
        android:id="@+id/secondFragment"
        android:layout_width="75dp"
        android:layout_height="75dp"
        app:srcCompat="@drawable/anniversary_tile" />

        <ImageButton
            android:id="@+id/thirdFragment"
            android:layout_width="75dp"
            android:layout_height="75dp"
            app:srcCompat="@drawable/shoutout_tile_100" />

        <ImageButton
            android:id="@+id/fourthFragment"
            android:layout_width="75dp"
            android:layout_height="75dp"
            app:srcCompat="@drawable/news_tile_100" />

        <ImageButton
            android:id="@+id/fifthFragment"
            android:layout_width="75dp"
            android:layout_height="75dp"
            app:srcCompat="@drawable/events_tile_100" />

    </LinearLayout>
    
    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="81dp"
        android:layout_below="@+id/viewPagerdash" />

</RelativeLayout>