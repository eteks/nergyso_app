<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <android.support.v4.view.ViewPager
        android:layout_width="wrap_content"
        android:layout_height="250dp"
        android:id="@+id/viewPagerdash"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true" />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/menuicons"
        android:orientation="horizontal"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/viewPagerdash">

        <ImageButton
            android:id="@+id/firstFragment"
            android:layout_width="45dp"
            android:layout_height="45dp"
            app:srcCompat="@drawable/birthday_blue"
            android:layout_alignParentTop="true"
            android:layout_toStartOf="@+id/secondFragment"
            android:layout_marginEnd="24dp" />

        <ImageButton
        android:id="@+id/secondFragment"
        android:layout_width="45dp"
        android:layout_height="45dp"
        app:srcCompat="@drawable/anniversary_blue"
            android:layout_alignParentTop="true"
            android:layout_toStartOf="@+id/thirdFragment"
            android:layout_marginEnd="22dp" />

        <ImageButton
            android:id="@+id/thirdFragment"
            android:layout_width="45dp"
            android:layout_height="45dp"
            app:srcCompat="@drawable/shoutout_blue"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginEnd="2dp"/>

        <ImageButton
            android:id="@+id/fourthFragment"
            android:layout_width="45dp"
            android:layout_height="45dp"
            app:srcCompat="@drawable/news_blue"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/thirdFragment"
            android:layout_marginStart="22dp" />

        <ImageButton
            android:id="@+id/fifthFragment"
            android:layout_width="45dp"
            android:layout_height="45dp"
            app:srcCompat="@drawable/events_blue"
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="32dp" />

    </RelativeLayout>
    
    <FrameLayout
        android:id="@+id/frameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/viewPagerdash"
        android:layout_marginTop="48dp" />

</RelativeLayout>